# http://editorconfig.org

# VS Code : (Ctrl+E) ext install editorconfig

root = true

[*]
charset      = utf-8
end_of_line  = crlf
indent_style = space
indent_size  = 2

[*.sh]
end_of_line  = lf

[*.ps1]
charset      = utf-8-bom
indent_size  = 4

[*.md]
trim_trailing_whitespace = false
